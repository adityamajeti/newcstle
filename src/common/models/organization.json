{
  "name": "Organization",
  "plural": "organization",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "tenantId": {
      "type": "string",
      "required": true,
      "id": true
    },
    "wso2Id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "locale": {
      "type": ["string"],
      "default": ["en-US"]
    },
    "timezone": {
      "type": "string",
      "default": "Asia/Kolkata"
    },
    "email": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "tags": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "regions": {
      "type": "hasMany",
      "model": "Region",
      "foreignKey": "tenantId",
      "options": {
        "nestRemoting": false
      }
    },
    "users": {
      "type": "hasMany",
      "model": "Users",
      "foreignKey": "tenantId",
      "options": {
        "nestRemoting": false
      }
    },
    "domains": {
      "type": "hasMany",
      "model": "Domain",
      "foreignKey": "tenantId",
      "options": {
        "nestRemoting": false
      },
      "through": "OrganizationDomain",
      "keyThrough": "domainId"
    }
  },
  "acls": [],
  "methods": {}
}
