{
  "name": "UserDomain",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "userId": {
      "type": "string",
      "required": true
    },
    "domainId": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "Users",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": false
      }
    },
    "domain": {
      "type": "belongsTo",
      "model": "Domain",
      "foreignKey": "domainId",
      "options": {
        "nestRemoting": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
